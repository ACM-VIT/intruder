{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "typescript",
        "airbnb-base"
    ],
    "parserOptions": {
        "project": "./server/tsconfig.json"
    },
    "rules": {
        "import/no-unresolved": "off",
        "@typescript-eslint/indent": ["error", 2],
        "import/prefer-default-export": "off"
    }
}
